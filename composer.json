{
    "name": "jacobtilly/larafort",
    "description": "A package to provide a simple way to connect Laravel to Fortnox.",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Tilly",
            "email": "dev@jacobtilly.com",
            "homepage": "https://jacobtilly.com"
        }
    ],
    "homepage": "https://github.com/jacobtilly/larafort",
    "keywords": ["Laravel", "LaraFort"],
    "require": {
        "illuminate/support": "^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "orchestra/testbench-core": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "JacobTilly\\LaraFort\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JacobTilly\\LaraFort\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JacobTilly\\LaraFort\\LaraFortServiceProvider"
            ],
            "aliases": {
                "LaraFort": "JacobTilly\\LaraFort\\Facades\\LaraFort"
            }
        }
    }
}
